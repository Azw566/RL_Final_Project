<?xml version="1.0" ?>
<!--
  Rescue Swarm Maze World

  A maze environment for drone exploration with ArUco markers as victims.

  Layout (20m x 20m):
  - Walls are 2m tall, 0.2m thick
  - Corridors are ~3m wide for drone navigation
  - Drones fly at 2.5m altitude (above walls for overview, in corridors for search)
  - 5 victims (ArUco markers) placed throughout the maze
  - 2 safety points for rescue dropoff

  Coordinate system: X positive = East, Y positive = North
-->
<sdf version="1.6">
  <world name="rescue_maze_world">

    <!-- Environment -->
    <include>
      <uri>model://sun</uri>
    </include>
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <!-- Physics settings for stable multi-drone simulation -->
    <physics type="ode">
      <max_step_size>0.004</max_step_size>
      <real_time_factor>1</real_time_factor>
      <real_time_update_rate>250</real_time_update_rate>
    </physics>

    <!-- ==================== MAZE WALLS ==================== -->
    <!-- Wall material: Gray concrete look -->

    <!-- Outer boundary walls -->
    <!-- North wall -->
